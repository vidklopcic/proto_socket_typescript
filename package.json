{
  "name": "proto_socket_typescript",
  "version": "1.0.6",
  "license": "MIT",
  "description": "Simple library that handles communication over websocket with 'proto_socket_django' backend.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc && copyfiles -u 1 src/**/*.js src/**/*.d.ts lib/",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vidklopcic/proto_socket_typescript.git"
  },
  "keywords": [
    "proto",
    "websocket"
  ],
  "author": "Vid Klopcic",
  "devDependencies": {
    "@types/uuid": "^8.3.3",
    "copyfiles": "^2.4.1",
    "prettier": "^2.5.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.5.2"
  },
  "dependencies": {
    "protobufjs": "^6.11.3",
    "rxjs": "^7.4.0",
    "uuid": "^8.3.2"
  }
}
